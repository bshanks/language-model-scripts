# s/(^|(?<=( |\t)))G\.($|(?=( |\t)))/GINGER/ig;
# s/(^|(?<=( |\t)))H\.($|(?=( |\t)))/HENRY/ig;
# s/(^|(?<=( |\t)))I\.($|(?=( |\t)))/INDIA/ig;
# s/(^|(?<=( |\t)))J\.($|(?=( |\t)))/JULIET/ig;
# s/(^|(?<=( |\t)))K\.($|(?=( |\t)))/KILO/ig;
# s/(^|(?<=( |\t)))L\.($|(?=( |\t)))/LIMA/ig;
# s/(^|(?<=( |\t)))M\.($|(?=( |\t)))/MICHAEL/ig;
# s/(^|(?<=( |\t)))N\.($|(?=( |\t)))/NOVEMBER/ig;
# s/(^|(?<=( |\t)))O\.($|(?=( |\t)))/OSCAR/ig;
# s/(^|(?<=( |\t)))P\.($|(?=( |\t)))/PETER/ig;
# s/(^|(?<=( |\t)))Q\.($|(?=( |\t)))/QUEBEC/ig;
# s/(^|(?<=( |\t)))R\.($|(?=( |\t)))/ROBBIE/ig;
# s/(^|(?<=( |\t)))S\.($|(?=( |\t)))/SIERRA/ig;
# s/(^|(?<=( |\t)))T\.($|(?=( |\t)))/TANGO/ig;
# s/(^|(?<=( |\t)))U\.($|(?=( |\t)))/UNIFORM/ig;
# s/(^|(?<=( |\t)))V\.($|(?=( |\t)))/VICTOR/ig;
# s/(^|(?<=( |\t)))W\.($|(?=( |\t)))/WHISKEY/ig;
