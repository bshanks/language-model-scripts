#!/usr/bin/perl

s/(^|(?<=( |\t)))QUOTATION_MARK($|(?=( |\t)))/VOICE_LITERAL_QUOTATION_MARK/ig;
s/(^|(?<=( |\t)))PERCENT_SIGN($|(?=( |\t)))/VOICE_LITERAL_PERCENT_SIGN/ig;
s/(^|(?<=( |\t)))AMPERSAND($|(?=( |\t)))/VOICE_LITERAL_AMPERSAND/ig;
s/(^|(?<=( |\t)))APOSTROPHE($|(?=( |\t)))/VOICE_LITERAL_APOSTROPHE/ig;
s/(^|(?<=( |\t)))LEFT_PARENS($|(?=( |\t)))/VOICE_LITERAL_LEFT_PARENS/ig;
s/(^|(?<=( |\t)))RIGHT_PARENS($|(?=( |\t)))/VOICE_LITERAL_RIGHT_PARENS/ig;
s/(^|(?<=( |\t)))COMMA($|(?=( |\t)))/VOICE_LITERAL_COMMA/ig;
s/(^|(?<=( |\t)))HYPHEN($|(?=( |\t)))/VOICE_LITERAL_HYPHEN/ig;
s/(^|(?<=( |\t)))PERIOD($|(?=( |\t)))/VOICE_LITERAL_PERIOD/ig;
s/(^|(?<=( |\t)))SLASH($|(?=( |\t)))/VOICE_LITERAL_SLASH/ig;
s/(^|(?<=( |\t)))COLON($|(?=( |\t)))/VOICE_LITERAL_COLON/ig;
s/(^|(?<=( |\t)))SEMICOLON($|(?=( |\t)))/VOICE_LITERAL_SEMICOLON/ig;
s/(^|(?<=( |\t)))QUESTION_MARK($|(?=( |\t)))/VOICE_LITERAL_QUESTION_MARK/ig;


s/(^|(?<=( |\t)))\"DOUBLE-QUOTE($|(?=( |\t)))/QUOTATION_MARK/ig;
s/(^|(?<=( |\t)))%PERCENT($|(?=( |\t)))/PERCENT_SIGN/ig;
s/(^|(?<=( |\t)))&AMPERSAND($|(?=( |\t)))/AMPERSAND/ig;
s/(^|(?<=( |\t)))'SINGLE-QUOTE($|(?=( |\t)))/APOSTROPHE/ig;
s/(^|(?<=( |\t)))\(LEFT-PAREN($|(?=( |\t)))/LEFT_PARENS/ig;
s/(^|(?<=( |\t)))\)RIGHT-PAREN($|(?=( |\t)))/RIGHT_PARENS/ig;
s/(^|(?<=( |\t))),COMMA($|(?=( |\t)))/COMMA/ig;
s/(^|(?<=( |\t)))-HYPHEN($|(?=( |\t)))/HYPHEN/ig;
s/(^|(?<=( |\t)))\.PERIOD($|(?=( |\t)))/PERIOD/ig;
s/(^|(?<=( |\t)))\/SLASH($|(?=( |\t)))/SLASH/ig;
s/(^|(?<=( |\t))):COLON($|(?=( |\t)))/COLON/ig;
s/(^|(?<=( |\t)));SEMI-COLON($|(?=( |\t)))/SEMICOLON/ig;
s/(^|(?<=( |\t)))\?QUESTION-MARK($|(?=( |\t)))/QUESTION_MARK/ig;

# s/(^|(?<=( |\t)))A\.($|(?=( |\t)))/ALFA/ig;
# s/(^|(?<=( |\t)))B\.($|(?=( |\t)))/BRAVO/ig;
# s/(^|(?<=( |\t)))C\.($|(?=( |\t)))/CHARLIE/ig;
